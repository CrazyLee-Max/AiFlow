{
    "kind": "selector",
    "name": "选择器节点",
    "description": "根据条件表达式进行分支选择",
    "template": {
        "id": "选择器节点ID",
        "name": "选择器",
        "kind": "selector",
        "outputPorts": ["应该是branches端口的集合"],
        "inputPorts": [],
        "inputParams": [],
        "outputParams": [],
        "branches": [
            {
                "port": "分支端口名",
                "conditionExpr": {
                    "kind": "logic",
                    "operator": "and|or",
                    "expresses": [
                        {
                            "kind": "compare",
                            "operator": "equal|notEqual|greaterThan|greaterThanEqual|lessThan|lessThanEqual|lengthGreaterThan|lengthGreaterThanEqual|lengthLessThan|lengthLessThanEqual|contain|notContain|isEmpty|notEmpty",
                            "leftExpress": {
                                "kind": "nodeVariable",
                                "nodeCode": "所选变量节点的code",
                                "variable": "所选变量名",
                                "variableId": "所选变量名ID"
                            },
                            "rightExpress": {
                                "kind": "stringConst|boolConst|numberConst|stringsConst|nodeVariable",
                                "value": "如果是常量，填写对应常量的值",
                                "nodeCode": "如果引用变量，填写节点code",
                                "variable": "如果引用变量，填写变量名",
                                "variableId": "如果引用变量，填写变量ID"
                            }
                        }
                    ]
                }
            },
            {
                "port": "default",
                "conditionExpr": null
            }
        ]
    },
    "notes": {
        "operators": {
            "logic": ["and", "or"],
            "compare": [
                "equal", "notEqual", 
                "greaterThan", "greaterThanEqual", 
                "lessThan", "lessThanEqual",
                "lengthGreaterThan", "lengthGreaterThanEqual",
                "lengthLessThan", "lengthLessThanEqual",
                "contain", "notContain",
                "isEmpty", "notEmpty"
            ]
        },
        "rightExpress_kinds": {
            "stringConst": "字符串常量",
            "boolConst": "布尔常量",
            "numberConst": "数字常量",
            "stringsConst": "字符串数组常量",
            "nodeVariable": "引用其他节点的变量"
        }
    }
}
